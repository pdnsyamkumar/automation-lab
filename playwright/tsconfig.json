{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "paths": {
      "@pages/*": ["./src/pages/*"],
      "@components/*": ["./src/components/*"],
      "@testData/*": ["./test-data/*"],
      "@utilities/*": ["./utilities/*"],
      "@matchers/*": ["./matchers/*"],
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
